extends layouts/home
include ../blocks/modules/page-default/page-default

include ../blocks/modules/home/main-banner/main-banner
include ../blocks/modules/home/main-program/main-program
include ../blocks/modules/home/main-events/main-events
include ../blocks/modules/home/main-partners/main-partners


block variables
  include ../data/data.pug
  - const title = "PFF"
 

block content 
  
  
    div#fullpage

      section.section.section-fp-home: +main-banner() 
      section.section.section-fp-home: +main-program() 
      section.section.section-fp-home: +main-events() 
      section.section.section-fp-home.partners-footer 
        .section-fp-wrap
          +main-partners() 
          footer
            include ../blocks/modules/common/footer/footer-min.pug 
       
        
 
     



block scripts
  script(src="/js/vendor.js") 
  script(src="/js/lib/splide.min.js") 
  script(src="/js/common.js") 
 
